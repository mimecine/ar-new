    "@astrojs/sitemap": "^3.4.1",
    "@astrojs/tailwind": "^6.0.2",
    "@extractus/oembed-extractor": "^4.0.8",
    "@fontsource-variable/fredoka": "^5.2.8",
    "@googlemaps/google-maps-services-js": "^3.4.1",
