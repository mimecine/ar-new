    "@iconify-json/material-symbols": "^1.2.19",
    "@iconify-json/material-symbols-light": "^1.2.19",
    "@iconify-json/mdi": "^1.2.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "concurrently": "^8.2.2",
